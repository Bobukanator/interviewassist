{
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": 1,
        "eol-last": 0,
        "vue/multi-word-component-names": [
            "error",
            {
                "ignores": [
                    "default"
                ]
            }
        ]
    }
}